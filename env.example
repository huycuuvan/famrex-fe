# Famarex Environment Configuration
# Copy this file to .env.local and update with your actual values

# AI Service API Configuration (SuperbAI)
NEXT_PUBLIC_AI_SERVICE_URL=https://aiapi.superbai.io/api/v1
NEXT_PUBLIC_AI_API_KEY=your_superbai_api_key_here

# User Management API Configuration  
NEXT_PUBLIC_USER_API_URL=https://api.famarex.com
NEXT_PUBLIC_USER_API_KEY=your_user_api_key_here

# Facebook API Configuration (for future integration)
NEXT_PUBLIC_FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=Famarex

# Security
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000
